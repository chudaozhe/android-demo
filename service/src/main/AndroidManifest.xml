<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.cuiwei.net.service">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".FirstService"
            android:enabled="true"
            android:exported="false" />
        <service
            android:name=".BindService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".BindServiceActivity" />

        <service
            android:name=".TwoService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".FirstIntentService"
            android:exported="false" />

        <activity android:name=".IntentServiceActivity" />

        <receiver
            android:name=".IntentServiceReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.CW_NOTIFY" />
            </intent-filter>
        </receiver>
        <receiver
            android:name=".orderReceiver.Receiver1"
            android:enabled="true"
            android:exported="true" >
            <intent-filter android:priority="2">
                <action android:name="android.intent.action.CW_ORDER_NOTIFY" />
            </intent-filter>
        </receiver>
        <receiver
            android:name=".orderReceiver.Receiver2"
            android:enabled="true"
            android:exported="true">
            <intent-filter android:priority="1">
                <action android:name="android.intent.action.CW_ORDER_NOTIFY" />
            </intent-filter>
        </receiver>

        <activity android:name=".orderReceiver.OrderActivity"></activity>
    </application>

</manifest>